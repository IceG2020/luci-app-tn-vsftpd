msgid "0 means no limitation"
msgstr "0 &mdash; без ограничений"

msgid "A message will be displayed when entering a directory."
msgstr "Сообщение отображается при входе в каталог."

msgid "ASCII mode"
msgstr "Режим ASCII"

msgid "Allow directory recursely list"
msgstr "Разрешить рекурсивный просмотр каталогов"

msgid "An actual local user to handle anonymous user"
msgstr ""
"Имя локального пользователя, от имени которого обеспечивается анонимный "
"доступ"

msgid "An actual local user to handle virtual users"
msgstr ""
"Имя локального пользователя, от имени которого обеспечивается доступ "
"виртуальным пользователям"

msgid "Anonymous Settings"
msgstr "Настройки анонимного доступа"

msgid "Anonymous User"
msgstr "Анонимный доступ"

msgid "Both download and upload"
msgstr "Для скачивания и загрузки"

msgid "Can not be zero, default is 3"
msgstr "Не может быть 0, по умолчанию 3"

msgid "Connection Settings"
msgstr "Настройки подключения"

msgid "Connection timeout"
msgstr "Таймаут подключения"

msgid "Data Port"
msgstr "Порт для данных"

msgid "Data connection timeout"
msgstr "Таймаут подключения для данных"

msgid "Directory message filename"
msgstr "Имя файла сообщения директории"

msgid "Disabled"
msgstr "Отключить"

msgid "Download only"
msgstr "Только для скачивания (download)"

msgid "Enable IPv4"
msgstr "Включить IPv4"

msgid "Enable IPv6"
msgstr "Включить IPv6"

msgid "Enable PASV mode"
msgstr "Включить режим PASV"

msgid "Enable PORT mode"
msgstr "Включить режим PORT"

msgid "Enable directory list"
msgstr "Разрешить просмотр списка директорий"

msgid "Enable directory message"
msgstr "Включить сообщения при входе в каталоги"

msgid "Enable download"
msgstr "Разрешить скачивание (download)"

msgid "Enable file log"
msgstr "Включить журналирование в файл"

msgid "Enable local users"
msgstr "Включить локальных пользователей"

msgid "Enable other rights"
msgstr "Разрешить прочие действия"

msgid "Enable syslog"
msgstr "Включить syslog журналирование"

msgid "Enable upload"
msgstr "Разрешить загрузку (upload)"

msgid "Enable write"
msgstr "Разрешить запись"

msgid "Enable write/mkdir"
msgstr "Разрешить запись и создание директорий"

msgid "Enabled"
msgstr "Включить"

